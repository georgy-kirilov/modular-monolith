{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "dotnet docker attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickRemoteProcess}",
            "pipeTransport": {
                "pipeProgram": "docker",
                "pipeArgs": ["exec", "-i", "modmontemplate-api", "sh", "-c"],
                "debuggerPath": "/vsdbg/vsdbg",
                "pipeCwd": "${workspaceFolder}",
                "quoteArgs": false
            },
            "sourceFileMap": {
                "/src": "${workspaceFolder}/backend"
            }
        }
    ]
}
